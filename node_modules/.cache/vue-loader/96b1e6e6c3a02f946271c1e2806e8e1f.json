tifyjs.com/images/cards/sun.png\"\n              alt=\"Sunny image\"\n              width=\"92\"\n            ></v-img>\n            <v-img :src=\"`~@/static/icons/${state_name_icon}.svg`\"></v-img>\n          </v-col>\n        </v-row>\n      </v-card-text>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\n\nexport default {\n  computed: {\n    ...mapGetters([\"city_info\"]),\n    description() {\n      let day = this.$moment(this.city_info.time).format(\"ddd\");\n      let time = this.$moment(this.city_info.time)\n        .format(\"hh:mm a\")\n        .toUpperCase();\n      return `${day}, ${time}, ${this.city_info.weather_state_name}`;\n    },\n    the_temp() {\n      return Number(this.city_info.the_temp).toFixed(0);\n    },\n    state_name_icon() {\n      const dict = [\n        \"cloudy,\",\n        \"cloud,\",\n        \"dust,\",\n        \"hail,\",\n        \"fog,\",\n        \"hot,\",\n        \"hurricane\",\n        \"lightning\",\n        \"meteor\",\n        \"na\",\n        \"rain\",\n        \"raindrop\",\n        \"raindrops\",\n        \"refresh\",\n        \"sandstorm\",\n        \"showers\",\n        \"sleet\",\n        \"smog\",\n        \"smoke\",\n        \"snow\",\n        \"starts\",\n        \"sunrise\",\n        \"sunset\",\n        \"thunderstorm\",\n        \"tornado\",\n        \"tsunami\",\n        \"windy\"\n      ];\n      let words = this.city_info.weather_state_name.split(\" \");\n      if (dict.indexOf(words[0]) !== -1) {\n        return `wi-${words[0]}`;\n      }\n\n      return `wi-na`;\n    }\n  },\n  mounted() {\n    console.log(this.$moment());\n  }\n};\n</script>\n\n<style lang=\"stylus\">\n@import ;\n</style>"]}]}